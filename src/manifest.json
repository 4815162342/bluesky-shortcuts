{
    "manifest_version": 2,
    "name": "bsky-shortcuts",
    "author": "Matt Buresh",
    "homepage_url": "https://github.com/mattburesh/bsky-shortcuts",
    "version": "0.3.1",
    "description": "Adds keyboard shortcuts to bsky.app",
    "icons": {},
